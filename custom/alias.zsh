# apt alias
alias apti="sudo apt-get install"
alias aptif="sudo apt-get install -y"
alias aptr="sudo apt-get remove"
alias apts="apt search"

# zsh alias
alias zshsrc="source $HOME/.zshrc"
alias zsheditsrc="nano $HOME/.zshrc"
alias zshalias="nano $HOME/.oh-my-zsh/custom/alias.zsh"
alias zshenv="nano $HOME/.oh-my-zsh/custom/env.zsh"
